{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
